package in.ksReddys1234.myweb.controller;
import java.util.list;
/*
 *
 */
public class calculator {
        /*
         * @param i
         * @param j
         * @return int
         */
        public int add(int i, int j) {
                return i+j;
        }
        public int multiply(int i, int j) {
                return i*j;
        }

        public int reminder(int i, int j) {
                return i%j;
        }
        
}